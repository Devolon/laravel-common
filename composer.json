{
    "name": "devolon/laravel-common",
    "description": "Devolon Payment provides utils in order to make development easier",
    "keywords": ["laravel", "utils"],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench-core": "^6.23",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Devolon\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devolon\\Common\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Devolon technical team",
            "homepage": "https://www.devolon.fi"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Devolon\\Common\\CommonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
